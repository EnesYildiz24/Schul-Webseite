{
  "name": "restmatcher",
  "version": "1.1.16",
  "description": "Matchers for Jest checking Responses returned via SuperTest",
  "author": "Jens von Pilgrim <jens.vonpilgrim@bht-berlin.de>",
  "license": "EPL-2.0",
  "homepage": "https://gitlab.bht-berlin.de/nodepackages/restmatcher",
  "repository": {
    "type": "git",
    "url": "https://gitlab.bht-berlin.de/nodepackages/restmatcher.git"
  },
  "main": "lib/restmatcher.js",
  "scripts": {
    "build": "tsc && copyfiles --up 1 -e ./src/**/*.ts ./src/**/*.* dist",
    "clean": "rimraf dist",
    "watch.build": "tsc --watch",
    "test": "JEST_JUNIT_SUITE_NAME=\"{filename}\" jest",
    "prepack": "npm run build"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "set-cookie-parser": "^2.6.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/set-cookie-parser": "^2.4.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^5.0.6",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
